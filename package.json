{
  "name": "telegraf-lambda-mysql-template",
  "version": "0.1.1",
  "description": "Template for creating future telegram bot apps",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "removePrismaExtras": "rm -rf node_modules/.prisma/client/libquery_engine-debian-openssl-1.1.x.so.node && rm -rf node_modules/@prisma/engines",
    "build": "npm run generateProxy && tsc -p .",
    "generateProxy": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "dev": "nodemon src/index.ts",
    "generate": "prisma generate",
    "deploy": "prisma migrate deploy",
    "migrate": "prisma migrate dev",
    "reset": "prisma migrate reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francisyzy/telegraf-lambda-mysql-template.git"
  },
  "author": "francisyzy",
  "bugs": {
    "url": "https://github.com/francisyzy/telegraf-lambda-mysql-template/issues"
  },
  "homepage": "https://github.com/francisyzy/telegraf-lambda-mysql-template#readme",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "dotenv": "^16.0.0",
    "qrcode-terminal": "^0.12.0",
    "telegraf": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/qrcode-terminal": "^0.12.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "ts-node": "^10.5.0",
    "typegram": "^3.8.0",
    "typescript": "^4.5.5"
  }
}
